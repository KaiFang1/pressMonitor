<Window x:Class="PressMonitor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PressMonitor"
        mc:Ignorable="d"
        Loaded="Function_Loaded"
        Title="MainWindow" Height="350" Width="525">

    <Grid>
        <ComboBox x:Name="Sensor1_comboBox" HorizontalAlignment="Left" Margin="37,138,0,0" VerticalAlignment="Top" Width="120" DropDownClosed="Sensor1_comboBox_DropDownClosed"/>
        <TextBox x:Name="presstextBox" HorizontalAlignment="Left" Height="24" Margin="284,140,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBlock x:Name="timeDateTextBlock" HorizontalAlignment="Left" Margin="157,63,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="启动监视" HorizontalAlignment="Left" Margin="181,243,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBox x:Name="WritetextBox" HorizontalAlignment="Left" Height="23" Margin="37,181,0,0" TextWrapping="Wrap" Text="Write指令" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
